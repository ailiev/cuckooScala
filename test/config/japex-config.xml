<testSuite name="Scala mutable map test suite"
           xmlns="http://www.sun.com/japex/testSuite">

  <param name="libraryDir" value="lib"/>
  <param name="japex.classPath" value="bin"/>
  <param name="japex.classPath" value="/kebab/minime/eclipse/configuration/org.eclipse.osgi/bundles/220/1/.cp/lib/scala-library.jar"/>
  <param name="japex.classPath" value="/home/sasho/lib/slf4j-api-1.5.10.jar"/>
  <param name="japex.classPath" value="/home/sasho/lib/slf4j-jdk14-1.5.10.jar"/>

  <param name="japex.warmupTime" value="10"/>
  <param name="japex.runTime" value="40"/>

  <!-- looks like the driver name has to be the class name -->
  <driver name="cuckoo.performance.japex.GetDriver">
    <param name="Description" value="Driver for map lookups"/>
    <param name="japex.DriverClass" value="cuckoo.performance.japex.GetDriver"/>
  </driver>

    <param name="timedBatchSize" value="500" />
    <param name="loadFactor" value="0.92" />    

  <testCase name="medium cuckoo">
    <param name="tableCapacity" value="20000"/>
    <param name="mapImpl" value="cuckoo" />
  </testCase>

  <testCase name="medium j.u.HashMap">
    <param name="tableCapacity" value="20000"/>
    <param name="mapImpl" value="java.util.HashMap" />
  </testCase>

</testSuite>
